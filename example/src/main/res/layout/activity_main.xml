<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:imports="com.hacknife.demo.Entity,entity;com.hacknife.demo.Entity,alisa;android.os.Bundle,bundle;">


    <TextView
        android:id="@+id/tv_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World ONE"
        android:textSize="20sp"
        app:bind="$ entity .index$>0? R.string.success:R.string.fail; $ alisa.index $>0? R.string.success:R.string.fail;"
        app:click="onUserClick($entity.list[0]$);onListener()"
        app:longClick="onUserLongClick($entity.list[0]$);ontest();"
        tools:ignore="MissingPrefix" />

    <TextView
        android:id="@+id/tv_test1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World TWO"
        android:textSize="20sp"
        app:bind="$bundle.USERNAME$;$alisa.list[3]$"
        tools:ignore="MissingPrefix" />

</LinearLayout>